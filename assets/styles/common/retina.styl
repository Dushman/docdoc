@media 
only screen and (-webkit-min-device-pixel-ratio: 2), 
only screen and (min-resolution: 192dpi) { 

    .index-img-wrapper i{
    	background-image:url('../img/retina/logo.png');
    	background-position:0 0;
    	width:153px;
    	height:49px;
    	background-size:153px 49px;
    	background-repeat:no-repeat;
    }

    #index-page div[data-role="footer"] > i{
    	background-image:url('../img/retina/doctor_image_bottom.png');
    	background-position:0 0;
    	background-size:64px 138px;
    	width:64px;
    	height:138px;
    	background-repeat:no-repeat;
    }

    #index-page ul.items-listing li.item-1 i{
    	background-image:url('../img/retina/notes-icon.png');
    	background-position:0 0;
    	background-size:14px 18.5px;
    	width:14px;
    	height:18.5px;
    	background-repeat:no-repeat;
    }

    #index-page ul.items-listing li.item-2 i{
    	background-image:url('../img/retina/doctors-icon.png');
    	background-position:0 0;
    	background-size:15px 18.5px;
    	width:15px;
    	height:18.5px;
    	background-repeat:no-repeat;
    }

    #index-page ul.items-listing li.item-3 i{
    	background-image:url('../img/retina/reviews-icon.png');
    	background-position:0 0;
    	background-size:14.5px 18.5px;
    	width:15.5px;
    	height:18.5px;
    	background-repeat:no-repeat;
    }

    .f-s a span{
    	background-image:url('../img/retina/doctor-bg.png');
    	background-position:0 0;
    	background-size:28px 28px;
    	width:28px;
    	height:28px;
    	background-repeat:no-repeat;
    }

    .f-m a span{
    	background-image:url('../img/retina/metro-bg.png');
    	background-position:0 0;
    	background-size:28px 28px;
    	width:28px;
    	height:28px;
    	background-repeat:no-repeat;
    }

    .find-doctor a.ui-link i{
    	background-image:url('../img/retina/lupa.png');
    	background-position:0 0;
    	background-size:20px 19.5px;
    	width:20px;
    	height:19.5px;
    	background-repeat:no-repeat;
    }

    #index-page div[data-role="footer"] .call-us i.iphone-bg{
    	background-image:url('../img/retina/iphone-bg.png');
    	background-position:0 0;
    	background-size:15px 25px;
    	width:15px;
    	height:25px;
    	background-repeat:no-repeat;
    }

    .fixed form.ui-filterable > i{
        background-image:url('../img/retina/orange-search-icon.png');
        background-position:0 0;
        background-size:20px 19.5px;
        width:20px;
        height:19.5px;
        background-repeat:no-repeat;
    }

    .fixed .fixed-inn .back-link{
        background-image:url('../img/retina/green-arrow.png');
        background-position:0 0;
        background-size:21.5px 15px;
        width:21.5px;
        height:15px;
        background-repeat:no-repeat;
    }

    .panel-btn{
        background-image:url('../img/retina/menu_btn.png');
        background-position:0 0;
        background-size:20px 13px;
        width:20px;
        height:13px;
        background-repeat:no-repeat;
    }

    .top-sort-block .location-link{
        background-image:url('../img/retina/location_btn.png');
        background-position:0 0;
        background-size:12.5px 18px;
        width:12.5px;
        height:18px;
        background-repeat:no-repeat;
    }

}