@font-face
    font-family 'Circe-regular'
    font-style normal
    font-weight 400
    src: local('☺'), 
     	url('fonts/Circe-Reg.woff') format('woff'),
     	url('fonts/Circe-Reg.ttf')  format('truetype'),
     	url('fonts/Circe-Reg.svg#Circe-Reg') format('svg')

