        padding:12px 43px;
    }

    .top-index-link a i{
        right:15px;
        top:17px;
    }

    .top-index-link{border-radius:18.5px;}

    .find-doctor{
        margin:0 22px;
    }

    .find-doctor a.ui-link{
        font-size:16px;
        letter-spacing:3px;
    }

    .find-doctor a.ui-link i{
        top:9px;
        right:18px;
    }

    #index-page div[data-role='footer']{
        padding-top:22px;
        padding-bottom:25px;
    }

    #index-page div[data-role='footer'] .call-us > strong{
