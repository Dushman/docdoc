#find-doctor div[data-role='footer']
	background-color #fff
	padding 0 1em

div[data-role='footer'] .fixed-inn
	position relative
	border-bottom none
	border-top 1px solid #dbdbdb
	overflow hidden

div[data-role='footer'] .fixed-inn span.filter-number
	text-transform uppercase
	font-size 14px
	float left
	line-height 49px

.panel-btn
	display block
	text-indent -9999em
	position absolute
	right 0
	top 50%
	margin-top -15px
	background transparent url('../img/pics/menu_btn.png') no-repeat 100% 50%
	width 30px
	height 30px
	z-index 2

p.description
	margin 0
	font-size 11px
	line-height 15px

.top-sort-block
	background-color #fff
	padding 1em 0
	overflow hidden

.top-sort-block > .left-block 
	float left

.top-sort-block > .left-block > a
	color #fc5412
	text-decoration none
	border-bottom 2px dotted #fc5412
	font-size 14px

.top-sort-block > .left-block > span 
	display inline-block
	border-width 6px
	border-style solid
	border-color transparent
	border-top-color #fc5412
	position relative
	left 6px
	top 6px

.top-sort-block .location-link
	float right
	sprite($s-location_btn)

.rightPanel.ui-panel-position-right.ui-panel-display-reveal
	-webkit-box-shadow none
	-moz-box-shadow none
	box-shadow none
	z-index 2001

.aside-foo
	position relative
	overflow hidden
	clear both
	padding .5em 1em 1.5em 0

.aside-foo a
	font-size 13px
	font-family 'Circe-bold'
	margin-left 12px

.aside-description
	position relative
	overflow hidden
	clear both
	padding-right 1em
	margin-bottom 1em

.aside-description p
	font-size 11px
	line-height 15px
	margin-bottom 5px

.aside-block 
	position relative
	overflow hidden
	clear both

.aside-block > strong:first-child
	display block
	clear both
	overflow hidden
	color #2cabad
	font-size 12px
	font-family 'Circe-bold'
	margin-bottom 12px

.aside-block ul,
.aside-block ul li
	position relative
	overflow hidden
	clear both

.aside-block ul li
	border-top 1px solid #d9d9d9
	font-size 13px
	
.aside-block ul li a,
.aside-block ul li a:hover,
.aside-block ul li a:active,
.aside-block ul li a:visited
	display block
	text-decoration none
	color #6f6f6f
	padding 15px 35px 15px 12px

#rightPanel .ui-panel-inner
	padding-right 0
	padding-bottom 0

.aside-header
	padding-right 1em
	position relative
	margin-bottom 1em
	display table
	width 100%

.aside-block ul li i
	display block
	sprite($s-arrow)
	position absolute
	top 50%
	margin-top -9px
	right 15px

.mb-15
	margin-bottom 15px

.logo-small
	float left
	sprite($s-logo_small)

.aside-header .city-dropdown-wrapper
	top 7px

.sort-doctor-wrapper
	float left
	position relative

.sort-doctor-wrapper .ui-select
	margin 0

.sort-doctor-wrapper .ui-select .ui-btn
	background transparent none
	border none

.sort-doctor-wrapper .ui-select .ui-btn > span:first-child
	color #fc5412
	text-decoration none
	border-bottom 2px dotted #fc5412
	font-size 14px
	font-weight 400
	font-family 'Circe-regular'
	text-shadow none

.sort-doctor-wrapper .ui-select .ui-btn > span:first-child:avtive,
.sort-doctor-wrapper .ui-select .ui-btn > span:first-child:focus
	text-shadow none

.top-sort-block span.triangle 
	display inline-block
	border-width 4px
	border-style solid
	border-color transparent
	border-top-color #fc5412
	position absolute
	right 17px
	top 7px

.mask-overlay 
	background rgba(0,0,0,.5)
	position absolute 
	left 0
	top 0
	bottom 0
	right 0
	z-index 1000
	display none

.padding-zero
	padding 0

.list-of-doctors
	position relative
	overflow hidden
	clear both
	padding 0 0 0 1em

.list-of-doctors > li
	position relative
	overflow hidden
	clear both
	border-top 1px solid #dedede
	padding 12px 30px 12px 0

.list-of-doctors > li:first-child
	border none

.list-of-doctors > li > a
	display block
	text-decoration none

.list-of-doctors > li > a .left-block
	float left
	margin-right 1em

.list-of-doctors > li > a .left-block .avatar-wrap
	overflow hidden
	clear both
	border 2px solid #257b7e
	background-color #257b7e
	border-radius 5px
	margin-bottom 8px
	text-align center
	width 65px
	height 65px

.list-of-doctors > li > a .left-block img
	vertical-align top
	display inline-block
	border-radius 3px

.list-of-doctors > li > a .left-block .rating-wrap
	overflow hidden
	clear both
	margin-bottom 3px

.list-of-doctors > li > a .left-block .rating-wrap strong:first-child
	float left
	color #fc5412
	font-size 12px
	line-height 12px
	font-weight 400

.list-of-doctors > li > a .left-block .rating-wrap ul
	float left
	clear none
	padding-left 5px
	padding-top 1px

.list-of-doctors > li > a .left-block .rating-wrap ul li
	float left
	margin 0 0 0 1px
	text-indent -9999em

.list-of-doctors > li > a .left-block .rating-wrap ul li.full
	sprite($s-star-full)

.list-of-doctors > li > a .left-block .rating-wrap ul li.empty
	sprite($s-star-empty)

.list-of-doctors > li > a .left-block .rating-wrap ul li:first-child
	margin-left 0

.list-of-doctors > li > a .left-block .qty-reviews
	display block
	clear both
	overflow hidden
	text-align center
	text-transform uppercase
	color #9a9a9a
	font-size 9px
	letter-spacing 2px

.list-of-doctors > li > a .right-block
	overflow hidden

.list-of-doctors > li > a .right-block h3.name
	margin 0 0 10px 0
	padding 0
	color #343131
	font-size 16px
	font-weight 400
	line-height 20px

.list-of-doctors > li > a .right-block .prof
	display block
	clear both
	color #18b1ac
	text-transform uppercase
	font-weight 400
	font-size 11px
	letter-spacing 1px
	margin-bottom 3px

.list-of-doctors > li > a .right-block .skills-wrap
	clear both
	overflow hidden
	margin-bottom 10px

.list-of-doctors > li > a .right-block .skills-wrap > span
	float left
	margin-left 15px
	color #9a9a9a
	font-size 11px

.list-of-doctors > li > a .right-block .skills-wrap > span:first-child
	margin-left 0

.list-of-doctors > li > a .right-block .skills-wrap > span.left b
	color #343131

.list-of-doctors > li > a .right-block .skills-wrap > span.right b
	color #fe5822

.list-of-doctors > li > a .right-block .skills-wrap > span.right b span
	font-family 'ALSRublRegular'
	font-weight 400

.address-wrap
	position relative
	overflow hidden
	clear both
	padding 0 0 0 15px
	color #9a9a9a
	font-size 11px
	height 13px
	line=height 13px

.address-wrap > i:first-child
	display block
	position absolute
	left 0
	top 0
	sprite($s-location-icon-green)

.list-of-doctors > li > a > i
	display block
	position absolute
	right 15px
	top 26px
	sprite($s-green-arrow)

#find-doctor .ui-select .ui-btn select,
.rightPanel .ui-select .ui-btn select
	height 25px
	min-height 25px
	top -5px
	font-size 12px
