#map_canvas
	width 100%
	height 100%
	min-height inherit
	position relative
	left 0
	top 0

.ui-content.map-content
	padding 0
	min-height inherit
	height 100%
	border-left 1px solid #999
	border-right 1px solid #999
	positon relative

#map-page div[data-role='footer']
	border-top 1px solid #999

#map-page div[data-role='header']
	border-bottom 1px solid #999

#map-page div[data-role='footer'] .fixed-inn,
#map-page div[data-role='header'] .fixed-inn
	border none

a.close-map
	display block
	position absolute
	right 10px
	top 20px
	width 31px
	height 31px
	sprite($s-map-btn)

a[href^='http://maps.google.com/maps']
	display none!important

.gmnoprint div
	background transparent!important

.gmnoprint a,
.gmnoprint span
	display none!important

.infobox-wrapper
	display none

.infobox-inn
	position relative
	width 210px
	overflow hidden
	background-color #fff
	border-radius 8px
	-webkit-box-shadow 0 3px 6px rgba(0,0,0,.3)
	-moz-box-shadow 0 3px 6px rgba(0,0,0,.3)
	box-shadow 0 3px 6px rgba(0,0,0,.3)

.infobox-inn .left-block
	float left

.infobox-inn .right-block
	overflow hidden
	padding 8px 5px 0 10px

.infobox-inn .right-block strong
	display block
	clear both
	font-size 11px
	line-height 14px
	margin-bottom 4px
	font-weight 400
	color #000
	font-family 'Circe-regular'

.infobox-inn .right-block em
	font-size 11px
	line-height 14px
	font-style normal
	color #848484
	font-family 'Circe-regular'
	font-weight 400

#infobox i.triangle
	display block
	position absolute
	left 50%
	bottom -17px
	margin-left 0px
	sprite($s-triangle-bottom)
	margin-left -20px